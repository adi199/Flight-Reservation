<% include main.ejs %>

<div class= " container-fluid col-md-6 col-md-offset-3 text-center">
  <form class="form-inline my-2 my-lg-0" action="/search" method="post">
      <input class="form-control mr-sm-3 text-center" type="search" placeholder="City" name="place">
      <input class="form-control mr-sm-3 text-center" type="date" placeholder="Check-in" name="check_in">
      <input class="form-control mr-sm-3 text-center" type="date" placeholder="Check-out" name="check_out">
      <select class="form-control" name="type">
        <option>Single room</option>
        <option>Double room</option>
        <option>Family room</option>
      </select>
      <button class="btn btn-outline-light my-2 my-sm-0 btn-dark" type="submit" value="Log In">Search</button>
  </form>
</div>
</body>
</html>
